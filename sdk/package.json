{
    "name": "@bitcoinbrisbane/block52",
    "version": "3.0.3",
    "description": "TypeScript SDK for Block52 Poker VM",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "clean": "rimraf dist",
        "prepublishOnly": "yarn clean && yarn build",
        "test": "jest",
        "lint": "eslint src --ext .ts,.tsx",
        "lint:fix": "eslint src --ext .ts,.tsx --fix"
    },
    "keywords": [
        "poker",
        "blockchain",
        "cosmos",
        "gaming",
        "sdk",
        "typescript"
    ],
    "author": "Block52",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/block52/poker-vm.git",
        "directory": "sdk"
    },
    "bugs": {
        "url": "https://github.com/block52/poker-vm/issues"
    },
    "homepage": "https://github.com/block52/poker-vm#readme",
    "devDependencies": {
        "@types/jest": "^30.0.0",
        "@types/node": "^22.7.6",
        "rimraf": "^6.0.1",
        "ts-jest": "^29.4.4",
        "ts-node": "^10.9.2",
        "tsup": "^8.3.5",
        "typescript": "^5.9.2"
    },
    "dependencies": {
        "@bufbuild/protobuf": "^2.10.0",
        "@cosmjs/crypto": "^0.36.1",
        "@cosmjs/encoding": "^0.36.1",
        "@cosmjs/proto-signing": "^0.36.1",
        "@cosmjs/stargate": "^0.36.1",
        "@keplr-wallet/types": "^0.12.138",
        "axios": "^1.12.2",
        "ethers": "^6.15.0",
        "long": "^5.3.2",
        "tslib": "^2.6.0"
    }
}
