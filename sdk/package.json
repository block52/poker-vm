{
    "name": "@block52/poker-vm-sdk",
    "version": "1.0.2",
    "description": "TypeScript SDK for Block52 Poker VM",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "yarn rollup -c",
        "build:types": "tsc --emitDeclarationOnly",
        "dev": "rollup -c -w",
        "clean": "rimraf dist",
        "prepublishOnly": "yarn clean && yarn build",
        "test": "jest",
        "lint": "eslint src --ext .ts,.tsx",
        "lint:fix": "eslint src --ext .ts,.tsx --fix",
        "proto:generate": "./regenerate-proto-types.sh",
        "proto:update-deps": "./update-dependencies.sh",
        "proto:full": "./regenerate-proto-types.sh && ./update-dependencies.sh"
    },
    "keywords": [
        "poker",
        "blockchain",
        "cosmos",
        "gaming",
        "sdk",
        "typescript"
    ],
    "author": "Block52",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/block52/poker-vm.git",
        "directory": "sdk"
    },
    "bugs": {
        "url": "https://github.com/block52/poker-vm/issues"
    },
    "homepage": "https://github.com/block52/poker-vm#readme",
    "devDependencies": {
        "@eslint/js": "^9.0.0",
        "@rollup/plugin-commonjs": "^28.0.1",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-node-resolve": "^15.3.0",
        "@rollup/plugin-typescript": "^12.1.1",
        "@types/jest": "^30.0.0",
        "@types/node": "^22.7.6",
        "@typescript-eslint/eslint-plugin": "^8.0.0",
        "@typescript-eslint/parser": "^8.0.0",
        "eslint": "^9.0.0",
        "jest": "^30.0.0",
        "rimraf": "^6.0.1",
        "rollup": "^4.28.1",
        "rollup-plugin-dts": "^6.1.1",
        "ts-jest": "^29.4.4",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.2"
    },
    "dependencies": {
        "@bufbuild/protobuf": "^2.10.0",
        "@cosmjs/crypto": "^0.32.4",
        "@cosmjs/encoding": "^0.32.4",
        "@cosmjs/proto-signing": "^0.32.4",
        "@cosmjs/stargate": "^0.32.4",
        "@keplr-wallet/types": "^0.12.0",
        "axios": "^1.12.2",
        "ethers": "^6.15.0",
        "events": "^3.3.0",
        "long": "^5.3.2",
        "tslib": "^2.6.0"
    }
}
