{
    "name": "@bitcoinbrisbane/block52",
    "version": "2.0.1",
    "description": "TypeScript SDK for Block52 Poker VM",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "yarn rollup -c",
        "build:types": "tsc --emitDeclarationOnly",
